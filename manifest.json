{
  "name": "Simple Greeting Dashboard",

  "version": "1.0",

  "description": "This Chrome extension greets the user each time they open a new tab",

  "manifest_version": 2,
  "incognito": "split",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    },

  "permissions": [
     "activeTab",
     "storage"
   ],
  "icons": {
      "128": "icon.png"
}
}
